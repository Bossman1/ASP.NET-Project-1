<!DOCTYPE HTML>
<html>
<head>
	@await Html.PartialAsync("_MetaPartial")
	@await Html.PartialAsync("_CssPartial")
</head>
<body class="right-sidebar is-preload">
	<div id="page-wrapper">

		<!-- Header Wrapper -->
		@await Html.PartialAsync("_HeaderPartial")

		<!-- Main Wrapper -->

		<div id="main-wrapper">
			<div class="wrapper style2">
				<div class="inner">
					<div class="container">
						<div class="row">
							@RenderBody()
						</div>
						</div>
					</div>
				</div>
			</div>
 
		 
	 
	 

		<!-- Footer Wrapper -->
		@await Html.PartialAsync("_FooterPartial")

	</div>

	<!-- Scripts -->
	@await Html.PartialAsync("_ScriptPartial")
</body>
</html>